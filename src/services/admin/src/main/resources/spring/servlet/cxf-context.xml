<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:jaxws="http://cxf.apache.org/jaxws"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
                     http://www.springframework.org/schema/beans/spring-beans.xsd
                     http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

 <import resource="classpath:META-INF/cxf/cxf.xml" />
 <import resource="classpath:META-INF/cxf/cxf-servlet.xml" />


 <jaxws:endpoint id="goodsAdd"  implementor="net.danvi.dmall.admin.webservice.service.GoodsAddServiceImpl"  address="/GoodsAdd" />

 <jaxws:endpoint id="categoryInfo"  implementor="net.danvi.dmall.admin.webservice.service.CategoryInfoServiceImpl"  address="/CategoryInfo" />

 <jaxws:endpoint id="brandInfo"  implementor="net.danvi.dmall.admin.webservice.service.BrandInfoServiceImpl"  address="/BrandInfo" />

</beans>